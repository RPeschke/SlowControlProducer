[CONNECTION]
baudRate =			9600
characterSize =		8
sendTwoStopBits =	false
enableParity =		false

[COMMANDS]
getVoltage_ch1.command =	V1?
getVoltage_ch1.regexp =		V1 [0-9]*\.[0-9]*
getVoltage_ch1.pattern =	$3

getVoltage_ch2.command =	V2?
getVoltage_ch2.regexp =		V2 [0-9]*\.[0-9]*
getVoltage_ch2.pattern =	$3

setVoltage_ch1 = V1 $
setVoltage_ch2 = V2 $
